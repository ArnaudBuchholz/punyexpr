{
  "name": "punyexpr",
  "version": "1.0.0",
  "description": "A minimalist - yet safe - eval helper",
  "main": "dist/punyexpr.js",
  "scripts": {
    "pretest": "rimraf .stryker-tmp",
    "test": "jest",
    "mutate": "stryker run",
    "lint": "standard --fix",
    "build": "uglifyjs punyexpr.js --compress --mangle --mangle-props reserved=['op','args','impl','str','tokenize','Error','version'] -o dist/punyexpr.js && node version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArnaudBuchholz/punyexpr.git"
  },
  "author": "Arnaud Buchholz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArnaudBuchholz/punyexpr/issues"
  },
  "homepage": "https://github.com/ArnaudBuchholz/punyexpr#readme",
  "devDependencies": {
    "@stryker-mutator/core": "^6.3.0",
    "@stryker-mutator/jest-runner": "^6.3.0",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "standard": "^17.0.0",
    "uglify-js": "^3.17.4"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "globals": [
      "punyexpr"
    ],
    "ignore": [
      "dist/punyexpr.js"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "punyexpr.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
